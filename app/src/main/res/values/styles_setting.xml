<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- setting文本信息-->
    <string name="opentext">开机启动</string>
    <string name="inform">通知图标</string>
    <string name="aboutustext">关于我们</string>
    <string name="helptext">帮助说明</string>
    <!-- TextView共有属性-->
    <style name="setting_style">
        <item name="android:layout_height">@dimen/height_action_bar</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:color">#000</item>
        <item name="android:textSize">30sp</item>
        <item name="android:padding">5dp</item>
    </style>
    <!-- buttons共有属性-->
    <style name="setting_buttons_style">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:background">@drawable/check_togglebutton</item>
        <item name="android:textOff"></item>
        <item name="android:textOn"></item>
    </style>
</resources>